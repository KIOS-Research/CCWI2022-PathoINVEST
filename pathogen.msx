[TITLE]


[OPTIONS]
AREA_UNITS     FT2
RATE_UNITS     HR
SOLVER     RK5
TIMESTEP     300
ATOL     0.001
RTOL     0.001

[SPECIES]
BULK CL2 MG 0.01 0.001
BULK P CFU 0.01 0.001
BULK C_FRA MG 0.01 0.001
BULK C_SRA MG 0.01 0.001

[COEFFICIENTS]
CONSTANT T 12
CONSTANT Kp 265.8
CONSTANT A 1
CONSTANT B 14 

[TERMS]
Km (1.5826e-04 * RE^0.88 )/ D
KWAL A*EXP(-B*CL2)
K_FAST 0.2808
K_SLOW 0.0071

[PIPES]
RATE CL2 -K_FAST*C_FRA*CL2-K_SLOW*C_SRA*CL2-((4/D)*(KWAL/(1+KWAL/Km)))*CL2
RATE P -Kp*P*CL2
RATE C_FRA -K_FAST*C_FRA*CL2
RATE C_SRA -K_SLOW*C_SRA*CL2

[TANKS]
RATE CL2 -K_FAST*C_FRA*CL2-K_SLOW*C_SRA*CL2
RATE P -Kp*P*CL2
RATE C_FRA -K_FAST*C_FRA*CL2
RATE C_SRA -K_SLOW*C_SRA*CL2

[SOURCES]

[QUALITY]

[PARAMETERS]

[PATTERNS]
[REPORT]
NODES ALL
LINKS ALL
